{
  "storage": {
    "contactInfoDb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "employeeInfoDb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "candidateInformationDb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "candidateInfoDb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "contactInfoLlambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "contactInfoDb",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    },
    "employeeInfoLambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "employeeInfoDb",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    },
    "candidateInfoLambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "candidateInfoDb",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    }
  },
  "api": {
    "contactInfo": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "contactInfoLlambda",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "employeeInfo": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "employeeInfoLambda",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "candidateInfo": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "candidateInfoLambda",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}